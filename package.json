{
  "name": "typopp",
  "version": "1.0.0",
  "description": "Google Drive document content analysis and editing tool",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "setup": "node setup.js"
  },
  "keywords": ["google-drive", "content-editing", "document-analysis"],
  "author": "Typopp Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 